          <div class="flex items-center">
            <div class="toggle-div flex items-center">
            <%= checkbox(:dark, "toggle" , phx_click: "toggle_dark" , value: @dark) %>
            <label for="dark_toggle" class="dark-toggle-label">
              Toggle Dark Mode
